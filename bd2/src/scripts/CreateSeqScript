    START WITH 1
    INCREMENT BY 1
    NOMAXVALUE;
CREATE SEQUENCE sq_dealers
    START WITH 1
    INCREMENT BY 1
    NOMAXVALUE;
CREATE SEQUENCE sq_delivers
    START WITH 1
    INCREMENT BY 1
    NOMAXVALUE;
CREATE SEQUENCE sq_orders
    START WITH 1
    INCREMENT BY 1
    NOMAXVALUE;
