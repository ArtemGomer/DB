DROP SEQUENCE sq_fee;
DROP SEQUENCE sq_dealers;
DROP SEQUENCE sq_delivered_goods;
DROP SEQUENCE sq_goods_type;
DROP SEQUENCE sq_delivers;
DROP SEQUENCE sq_orders;
DROP SEQUENCE sq_cells;

CREATE SEQUENCE sq_fee
    START WITH 1
    INCREMENT BY 1
    NOMAXVALUE;
